  <WorkerRole name="RssAggregator.Role.HighFrequency" vmsize="Small">
    <Runtime executionContext="elevated" />
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="HighFrequencyRefreshPeriod" />
      <Setting name="HighFrequencyRefreshSplit" />
      <Setting name="FeedLibraryUrl" />
    </ConfigurationSettings>
  </WorkerRole>
  
  
  <WebRole name="RssAggregator.WebRole" vmsize="ExtraSmall">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="LowFrequencyHttpWebRequestTimeout" />
      <Setting name="FeedLibraryUrl" />
    </ConfigurationSettings>
  </WebRole>
  
  
