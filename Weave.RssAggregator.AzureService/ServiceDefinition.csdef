<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Weave.RssAggregator.AzureService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-03.2.0">
  <WorkerRole name="RssAggregator.WorkerRole" vmsize="ExtraSmall">
    <Runtime executionContext="elevated" />
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="LowFrequencyHttpWebRequestTimeout" />
      <Setting name="FeedLibraryUrl" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>