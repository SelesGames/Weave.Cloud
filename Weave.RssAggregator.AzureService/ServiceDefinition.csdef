<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Weave.RssAggregator.AzureService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-10.1.8">
  <WorkerRole name="RssAggregator.WorkerRole" vmsize="ExtraSmall">
    <Runtime executionContext="elevated" />
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="LowFrequencyHttpWebRequestTimeout" />
      <Setting name="HighFrequencyRefreshPeriod" />
      <Setting name="HighFrequencyRefreshSplit" />
      <Setting name="FeedLibraryUrl" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
  </WorkerRole>
  <WorkerRole name="RssAggregator.Role.HighFrequency" vmsize="Small">
    <Runtime executionContext="elevated" />
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="HighFrequencyRefreshPeriod" />
      <Setting name="HighFrequencyRefreshSplit" />
      <Setting name="FeedLibraryUrl" />
    </ConfigurationSettings>
    <Endpoints>
      <InternalEndpoint name="Endpoint1" protocol="tcp" port="9352" />
    </Endpoints>
  </WorkerRole>
</ServiceDefinition>